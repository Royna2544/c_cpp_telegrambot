{
    "name": "Eureka Kernel",
    "repo": {
        "url": "https://github.com/Roynas-Android-Playground/kernel_samsung_exynos7885-priv",
        "branch": "EurekaV2"
    },
    "arch": "arm64",
    "type": "Image",
    "toolchains": {
        "clang": true,
        "llvm_binutils": true,
        "llvm_ias": true
    },
    "anykernel": {
        "enabled": true,
        "location": "AnyKernel3"
    },
    "defconfig": {
        "scheme": "exynos7885_defconfig",
        "devices": [
            "a10",
            "a20",
            "a20e",
            "a30",
            "a30s",
            "a40",
            "jackpotlte"
        ]
    },
    "fragments": [
        {
            "name": "Device Specific",
            "scheme": "{device}.config",
            "depends": [],
            "description": "Enable device target support",
            "default_enabled": true
        },
        {
            "name": "NO KSU",
            "scheme": "noksu.config",
            "depends": [],
            "description": "Disable KSU",
            "default_enabled": false
        }
    ],
    "env": [
        {
            "name": "SOME_ARCH",
            "value": "arm64"
        }
    ]
}